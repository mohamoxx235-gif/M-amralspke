            6: { a: "news readers", b: "journalists", c: "economists", d: "meteorologists" },
            7: { a: "downpour", b: "flooding", c: "drought", d: "draught" },
            8: { a: "honesty", b: "equity", c: "etiquette", d: "equivalent" },
            9: { a: "tangible", b: "virtual", c: "technical", d: "hardware" },
            10: { a: "helpless", b: "dull", c: "defeating", d: "pessimistic" },
