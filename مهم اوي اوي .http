    <script>
        // الإجابات الصحيحة
        const correctAnswers = {
            1: "a", 2: "c", 3: "d", 4: "b", 5: "a",
            6: "d", 7: "c", 8: "b", 9: "a", 10: "d",
            11: "c", 12: "b", 13: "a", 14: "b", 15: "a",
            16: "c", 17: "b", 18: "d", 19: "a", 20: "d",
            21: "b", 22: "c", 23: "d", 24: "a", 25: "c",
            26: "b", 27: "a", 28: "d", 29: "c", 30: "a"
        };

        let userAnswers = {};
        let studentName = "";

        // تحديث شريط التقدم
        function updateProgress() {
            const answeredQuestions = Object.keys(userAnswers).length;
            const totalQuestions = 30;
            const percentage = (answeredQuestions / totalQuestions) * 100;
            
            // تحديث العداد
            document.getElementById('progressCount').textContent = `${answeredQuestions}/${totalQuestions}`;
            document.getElementById('progressCount2').textContent = `${answeredQuestions}/${totalQuestions}`;
            
            // تحديث شريط التقدم
            document.getElementById('progressFill').style.width = `${percentage}%`;
            document.getElementById('progressFill2').style.width = `${percentage}%`;
            
            // تحديث علامة الإجابة على الأسئلة
            for (let i = 1; i <= totalQuestions; i++) {
                const questionElement = document.getElementById(`question${i}`);
                if (userAnswers[i]) {
                    if (questionElement) questionElement.classList.add('answered');
                } else {
                    if (questionElement) questionElement.classList.remove('answered');
                }
            }
        }

        // إدارة الصفحات
        function showPage(pageNumber) {
            document.getElementById('page1').style.display = 'none';
            document.getElementById('page2').style.display = 'none';
            document.getElementById('resultsPage').style.display = 'none';
            
            document.getElementById('page' + pageNumber).style.display = 'block';
            checkAllQuestionsAnswered();
        }

        // التحقق من إجابة جميع الأسئلة
        function checkAllQuestionsAnswered() {
            const totalQuestions = Object.keys(correctAnswers).length;
            const answeredQuestions = Object.keys(userAnswers).length;
            const submitBtn = document.getElementById('submitBtn');
            
            submitBtn.disabled = answeredQuestions !== totalQuestions;
        }

        // أحداث الخيارات
        document.querySelectorAll('.option').forEach(option => {
            option.addEventListener('click', function() {
                const questionNum = this.getAttribute('data-question');
                const optionLetter = this.getAttribute('data-option');
                const answerText = this.getAttribute('data-answer');
                
                // إزالة التحديد من جميع خيارات هذا السؤال
                document.querySelectorAll(`.option[data-question="${questionNum}"]`).forEach(opt => {
                    opt.classList.remove('selected');
                });
                
                // تحديد الخيار المختار
                this.classList.add('selected');
                
                // ملء المساحة الفارغة
                const blankSpace = document.querySelector(`.blank-space[data-question="${questionNum}"]`);
                blankSpace.classList.add('filled');
                blankSpace.setAttribute('data-answer', answerText);
                
                // حفظ إجابة المستخدم
                userAnswers[questionNum] = optionLetter;
                
                updateProgress();
                checkAllQuestionsAnswered();
            });
        });

        // زر التسليم
        document.getElementById('submitBtn').addEventListener('click', function() {
            studentName = document.getElementById('studentName').value.trim();
            
            if (!studentName) {
                alert("⚠️ الرجاء إدخال اسمك الثلاثي أولاً");
                return;
            }

            // حساب النتائج
            let correctCount = 0;
            let wrongAnswers = [];

            for (let i = 1; i <= 30; i++) {
                if (userAnswers[i] === correctAnswers[i]) {
                    correctCount++;
                } else {
                    wrongAnswers.push({
                        question: i,
                        userAnswer: userAnswers[i],
                        correctAnswer: correctAnswers[i]
                    });
                }
            }

            const percentage = Math.round((correctCount / 30) * 100);

            // عرض النتائج
            document.getElementById('studentNameDisplay').textContent = studentName;
            document.getElementById('correctAnswers').textContent = correctCount;
            document.getElementById('incorrectAnswers').textContent = 30 - correctCount;
            document.getElementById('percentage').textContent = `${percentage}%`;
            document.getElementById('scorePercent').textContent = `${percentage}%`;

            // تحديث الدائرة البيانية
            const scoreCircle = document.querySelector('.score-circle');
            scoreCircle.style.background = `conic-gradient(#4caf50 0% ${percentage}%, #e0e0e0 ${percentage}% 100%)`;
            // عرض الإجابات الخاطئة
            if (wrongAnswers.length > 0) {
                const wrongAnswersList = document.getElementById('wrongAnswersList');
                wrongAnswersList.innerHTML = '';
                
                wrongAnswers.forEach(wrong => {
                    const wrongItem = document.createElement('div');
                    wrongItem.className = 'wrong-answer-item';
                    wrongItem.innerHTML = `
                        <strong>السؤال ${wrong.question}:</strong><br>
                        إجابتك: <span class="incorrect">${wrong.userAnswer}</span><br>
                        الإجابة الصحيحة: <span class="correct">${wrong.correctAnswer}</span>
                    `;
                    wrongAnswersList.appendChild(wrongItem);
                });
                
                document.getElementById('wrongAnswersSection').style.display = 'block';
            }

            showPage('results');
        });

        // زر إعادة الامتحان
        document.getElementById('restartBtn').addEventListener('click', function() {
            userAnswers = {};
            studentName = "";
            
            document.querySelectorAll('.option').forEach(option => {
                option.classList.remove('selected');
            });
            
            document.querySelectorAll('.blank-space').forEach(space => {
                space.classList.remove('filled');
                space.removeAttribute('data-answer');
            });
            
            document.getElementById('studentName').value = "";
            document.getElementById('wrongAnswersSection').style.display = 'none';
            
            updateProgress();
            showPage(1);
        });
        // بدء الصفحة الأولى
        showPage(1);
        updateProgress();

        /* الكود الجديد - ضعه هنا */
        // نص الأسئلة الكامل
        const questions = {
