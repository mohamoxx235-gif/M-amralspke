            13: { a: "community", b: "group", c: "region", d: "association" },
            14: { a: "broadcast", b: "forecast", c: "expects", d: "predictive" },
            15: { a: "gases", b: "steams", c: "fuels", d: "elements" },
            16: { a: "detached house", b: "glasshouse", c: "greenhouse", d: "smart house" },
            17: { a: "excess", b: "extreme", c: "accessible", d: "intensely" },
            18: { a: "friendly", b: "acceptable", c: "rational", d: "sustainable" },
            19: { a: "creativity", b: "innovative", c: "original", d: "ingenious" },
            20: { a: "plantation", b: "realisation", c: "communication", d: "implementation" },
            21: { a: "initial", b: "initiative", c: "acceptance", d: "document" },
            22: { a: "announce", b: "declare", c: "emphasize", d: "make sure" },
            23: { a: "title", b: "subject", c: "sector", d: "issue" },
            24: { a: "environmental", b: "technical", c: "climate", d: "meteorological" },
            25: { a: "sequels", b: "influences", c: "consequences", d: "affects" },
            26: { a: "sources", b: "resources", c: "rights", d: "beings" },
