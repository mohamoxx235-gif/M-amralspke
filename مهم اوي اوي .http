            29: { a: "climates", b: "meteorologists", c: "hurricanes", d: "washing water" },
            30: { a: "threaten", b: "empower", c: "defend", d: "risk" }
        };

        // دالة لعرض التفاصيل
        function showDetailedResults() {
            const answersGrid = document.getElementById('answersGrid');
            answersGrid.innerHTML = '';

            for (let i = 1; i <= 30; i++) {
                const isCorrect = userAnswers[i] === correctAnswers[i];
                
                const answerCard = document.createElement('div');
                answerCard.className = `answer-card ${isCorrect ? 'correct' : 'incorrect'}`;
                
                answerCard.innerHTML = `
                    <div class="question-number">السؤال ${i}</div>
                    <div style="margin-bottom: 8px; font-size: 14px;">${questions[i]}</div>
                    <div>إجابتك: <span class="${isCorrect ? 'correct-answer' : 'student-answer'}">${optionsText[i][userAnswers[i]]}</span></div>
                    ${!isCorrect ? `<div>الإجابة الصحيحة: <span class="correct-answer">${optionsText[i][correctAnswers[i]]}</span></div>` : ''}
                `;

                answersGrid.appendChild(answerCard);
            }
        }
